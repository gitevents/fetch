{
  "name": "gitevents-fetch",
  "version": "1.0.0",
  "description": "Fetch events and talks from GitHub Issues",
  "keywords": [
    "gitevents",
    "events",
    "issue ops",
    "fetch",
    "github",
    "action"
  ],
  "homepage": "https://github.com/gitevents",
  "bugs": {
    "url": "https://github.com/gitevents/fetch/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitevents/fetch.git"
  },
  "license": "MIT",
  "author": "Patrick Heneise <patrick@zentered.co> (https://zentered.co)",
  "contributors": [
    {
      "name": "Patrick Heneise",
      "url": "https://zentered.co",
      "author": true
    }
  ],
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run lint && npm run test",
    "format": "prettier --write .",
    "postinstall": "husky install",
    "lint": "eslint .",
    "test": "node --test --experimental-loader ./test/gql-loader.js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.{js}": [
      "eslint --cache --fix"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@octokit/auth-app": "^8.1.1",
    "@octokit/auth-token": "^6.0.0",
    "@octokit/graphql": "^9.0.2",
    "@zentered/issue-forms-body-parser": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.0",
    "@vercel/ncc": "^0.38.4",
    "commitlint": "^20.1.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.0",
    "vite": "^7.1.12",
    "vite-plugin-graphql-loader": "^4.0.4"
  }
}
